<% @spotify_songs.each do |track|%>

<%= track.name%>
<%artists = ""%>
<% track.artists.each do |artist|%>
	<%artists += "#{artist.name},"  %>
	<%= artist.name  %>
	<%end%>

<%= form_tag playlists_path , method: :patch  %>
    <%= hidden_field_tag "song[title]", track.name%>
     <%= hidden_field_tag "song[artist_name]", artists%>

    <%= submit_tag  "Add Song"%>



	
		<%end%>